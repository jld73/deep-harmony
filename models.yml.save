preprocessing:
    sequence:
        format: space
        most_common: 30000
        sequence_length_limit: 1000

input_features:
    -
        name: sop
        type: sequence
        encoder: rnn
        representation: sparse

output_features:
    -
        name: bas
        type: text
        decoder: generator

training:
    batch_size: 96
    epochs: 1000
    early_stop: 10
